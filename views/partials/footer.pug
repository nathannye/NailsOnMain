footer 
  .footerHeadingContainer
    h2= footer.data.heading
    a.solidButton.nonNavLink(href='tel:' + contact.data.phone_number)= contact.data.phone_number
  .footerContentContainer
    .footerCols
      .footerLocationCol
        h3 Where we're at:
        a(href='https://goo.gl/maps/xSZVvVL7o4mEsprd9', target='_blank')
          p= contact.data.address_line_1
          br 
          p= contact.data.address_line_2
      .footerMenuCol
        h3 Menu
        ul.footerMenuLinksContainer
          each link in menu.data.links
            li
              a.nakedLink(href=link.link.url, title='call' + link.link.url)= link.link_label

      .footerSocialCol
        h3 Social
        ul.footerSocialLinksContainer
          each link in contact.data.social_media
            li
              a.nakedLink(href=link.social_media_link.url, target='_blank')= link.social_media_label
    .footerLowerInfo
      a.nakedLink.credit(
        href='https://nye.design/',
        target='_blank',
        rel='noreferrer'
      ) Website by Nathan Nye
      a.nakedLink.privacyPolicy(href='/privacy-policy') Privacy Policy
      p.copyright= 'Copyright' + ' Â© ' + year
